    <div>
		<!-- Vue實例的程式碼 -->
		<script>
			const posts = []

			const SECTIONS_YL = "宜蘭"
			const SECTIONS_HL = "花蓮"
			const SECTIONS_TT = "台東"

			new Vue({
				el: '#app1',
				data: {
					posts,
					// results: [],
					sections: SECTIONS_YL.split(', '),
					section: '宜蘭'
				},
				filter: {
					formatted(value) {
						return ' 以下是 ${value} 類別的貼文 '
					}
				},

				computed: {
					results() {
						let posts = this.posts
							.slice()
							.filter(post => post.Category === this.section)
						let i, j, chunkedArray = [], chunk = 20;
						for (i = 0, j = 0; i < posts.length; i += chunk, j++) {
							chunkedArray[j] = posts.slice(i, i + chunk);
						}
						return chunkedArray
					}
				},

				mounted() {
					axios.get('file/TGE-411-YilanHotel.json').then((response) => {
						this.posts = response.data;
						this.getPostsByCategory(this.section)
					}).catch(error => { console.log(error); });
				},

			});

